srcdir=@srcdir@
VPATH=$(srcdir)

CC=@CC@
DEPEND=@DEPEND@

CFLAGS=-Wall -O2 -g @CFLAGS@ -I..
OBJS=grab-ng.o @NG_GRAB@				\
	color_common.o color_packed.o color_lut.o	\
	color_yuv2rgb.o					\
	mjpeg.o						\
	writefile.o writeavi.o writeqt.o

all build: libng.a

libng.a: $(OBJS)
	rm -f $@
	ar -r $@ $(OBJS)
	ranlib $@

clean:
	rm -f libng.a $(OBJS)

distclean realclean: clean
	rm -f Makefile *~ *.bak

install:

depend dep:
	$(DEPEND) -- $(CFLAGS) -- *.c

